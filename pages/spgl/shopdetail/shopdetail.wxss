.label{
  display: inline-block;
  font-size: 14px;
  flex: 2.6;
  text-align: right;
  line-height: 40px;
  margin-right: 30px;
}
.detail_content{
  padding: 10px;
  box-sizing: border-box;
}
.header{
  height: 34px;
  line-height: 34px;
  color: #8d8d8d;
  background: #f1f1f1;
  font-size: 16px;
  text-align: center;
}
.psbtn{
  margin-top: 4px;
  font-size: 16px;
}
.psbtn1{
  margin-top: 20px;
}
.form_item{
  display: flex;
  margin-top: 14px;
  font-size: 16px;
}
.section__title{
  color: #606266;
  font-size: 16px;
}
.good_detail{
  background: #fff;
}
.psinput{
  flex: 7;
  border-radius: 4px;
  height: 40px;
  line-height: 40px;
  margin-right: 20px;
  padding-left: 10px;
  border: 1px solid #dcdfe6;
}
